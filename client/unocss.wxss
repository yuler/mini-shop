/* layer: default */
.invisible{visibility:hidden;}
.fixed{position:fixed;}
.bottom-0{bottom:0;}
.left-0{left:0;}
.grid{display:grid;}
.grid-cols-2{grid-template-columns:repeat(2,minmax(0,1fr));}
.m-auto{margin:auto;}
.mb-2{margin-bottom:16rpx;}
.mt-2{margin-top:16rpx;}
.mt-4{margin-top:32rpx;}
.block{display:block;}
.h-18{height:4.5rem;}
.h-20{height:160rpx;}
.h-25{height:200rpx;}
.h-600rpx{height:600rpx;}
.h-full{height:100%;}
.w-20{width:5rem;}
.w-25{width:6.25rem;}
.w-full{width:100%;}
.flex{display:flex;}
.flex-col{flex-direction:column;}
.items-center{align-items:center;}
.justify-center{justify-content:center;}
.justify-between{justify-content:space-between;}
.gap-2{grid-gap:16rpx;gap:0.5rem;}
.gap-4{grid-gap:1rem;gap:32rpx;}
.overflow-hidden{overflow:hidden;}
.rounded{border-radius:0.25rem;}
.bg-black{--un-bg-opacity:1;background-color:rgba(0,0,0,var(--un-bg-opacity));}
.bg-white{--un-bg-opacity:1;background-color:rgba(255,255,255,var(--un-bg-opacity));}
.px-2{padding-left:16rpx;padding-right:0.5rem;}
.px-4{padding-left:1rem;padding-right:32rpx;}
.py-2{padding-top:0.5rem;padding-bottom:16rpx;}
.pb-10{padding-bottom:2.5rem;}
.pb-20{padding-bottom:160rpx;}
.text-center{text-align:center;}
.text-lg{font-size:36rpx;line-height:1.75rem;}
.text-sm{font-size:28rpx;line-height:1.25rem;}
.text-xl{font-size:40rpx;line-height:1.75rem;}
.font-bold{font-weight:700;}
.lh-15{line-height:120rpx;}
.text-gray-500{--un-text-opacity:1;color:rgba(107,114,128,var(--un-text-opacity));}
.text-gray-800{--un-text-opacity:1;color:rgba(31,41,55,var(--un-text-opacity));}
.text-white{--un-text-opacity:1;color:rgba(255,255,255,var(--un-text-opacity));}